{
  "name": "starter-kit-react",
  "version": "0.0.1",
  "description": "Starter kit for a React App",
  "main": "dist/index.js",
  "repository": "https://github.com/TouchtechLtd/starter-kit-react.git",
  "author": "Thram <thramposo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack -p",
    "build-storybook": "build-storybook",
    "cover": "nyc yarn test:single && yarn report",
    "cz": "git add . && git cz",
    "lint": "eslint src --ext .jsx,.js",
    "lint:test": "eslint __test__ --ext .js",
    "prebuild": "rimraf dist",
    "precz": "yarn test:single",
    "report": "nyc report --reporter=lcov",
    "tape": "tape-watch -r babel-register __test__/*.test.js",
    "test": "yarn lint && yarn tape -- -p | faucet",
    "test:single": "yarn lint && yarn tape -- --once -p | faucet",
    "start": "webpack-dev-server --host 0.0.0.0 --port 8080",
    "storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@storybook/react": "^3.0.0-alpha.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "faucet": "^0.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "husky": "^0.13.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "tape": "^4.6.3",
    "tape-watch": "^2.3.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "glamor": "^2.20.25",
    "glamorous": "^3.14.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "pure-components": "^1.1.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0"
  }
}
