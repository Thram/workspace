{
  "name": "starter-kit-react",
  "version": "0.0.1",
  "description": "Starter kit for a React App",
  "main": "dist/index.js",
  "repository": "https://github.com/TouchtechLtd/starter-kit-react.git",
  "author": "Thram <thramposo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production webpack -p",
    "build:gzip": "NODE_ENV=production webpack -p --env.compress",
    "build:dev": "webpack",
    "build:docs": "build-storybook -o ${PWD}/docs",
    "precz": "yarn test",
    "cz": "git add . && git cz",
    "lint": "eslint src --ext .jsx,.js",
    "lint:test": "eslint __test__ --ext .js",
    "test": "yarn lint && jest",
    "snap": "yarn lint && jest -u",
    "start": "webpack-dev-server --host 0.0.0.0 --port 8080",
    "storybook": "start-storybook -p 6006"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss|sass|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.0.1",
    "@storybook/addon-info": "^3.0.1",
    "@storybook/addon-knobs": "^3.0.1",
    "@storybook/addon-options": "^3.0.1",
    "@storybook/addon-storyshots": "^3.0.0",
    "@storybook/react": "^3.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "commitizen": "^2.9.6",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "husky": "^0.13.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "preload-webpack-plugin": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "storybook-addon-jsx": "^4.1.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "glamor": "^2.20.25",
    "glamorous": "^3.22.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.3"
  }
}