{
  "name": "ZenWorkspace",
  "version": "0.0.3",
  "description": "Electron app to manage multiple webapps",
  "main": "index.js",
  "repository": "https://github.com/Thram/zen-workspace",
  "author": "Thram <thramposo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "install-app-deps",
    "release": "build -m -p always",
    "dist": "NODE_ENV=production yarn build -- -p && build",
    "prebuild": "rimraf release",
    "predist": "rimraf dist",
    "build": "NODE_PATH=. webpack",
    "build:gzip": "yarn build -- -p --env.compress",
    "precz": "yarn test",
    "cz": "git add . && git cz",
    "lint": "eslint src --ext .jsx,.js",
    "lint:test": "eslint __test__ --ext .js",
    "test": "yarn lint && jest",
    "snap": "yarn lint && jest -u",
    "electron": "electron .",
    "chrome-extension": "repackage-chrome-extension \"~/Library/Application Support/Google/Chrome/Default/Extensions/\"",
    "dev-server": "webpack-dev-server --host 0.0.0.0 --port 8080",
    "start:prod": "NODE_ENV=production npm-run-all \"build -- -p \" electron",
    "start": "NODE_ENV=development npm-run-all --parallel electron dev-server "
  },
  "build": {
    "appId": "thram.zenWorkspace",
    "asar": false,
    "files": [
      ".babelrc",
      "main.js",
      "index.js",
      "menu.js",
      "release",
      "remote",
      "extensions"
    ],
    "dmg": {
      "contents": [
        {
          "x": 510,
          "y": 150
        },
        {
          "x": 640,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": ["AppImage", "deb"]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Thram",
        "repo": "zen-workspace"
      }
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss|sass|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@getstation/repackage-chrome-extension-for-electron": "^0.1.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "commitizen": "^2.9.6",
    "compression-webpack-plugin": "^1.0.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.19.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.0.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "preload-webpack-plugin": "^1.2.3",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "base64-img": "^1.0.3",
    "electron-log": "^2.2.7",
    "electron-updater": "^2.8.2",
    "glamor": "^3.0.0-2",
    "glamorous": "^4.1.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-alpha.19",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.3",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.1.0",
    "validator": "^8.0.0",
    "whatwg-fetch": "^2.0.3",
    "wildcard": "^1.1.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
